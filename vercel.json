{
  "buildCommand": "cd next_frontend && npm install && npm run build",
  "outputDirectory": "next_frontend/.next",
  "devCommand": "cd next_frontend && npm run dev",
  "installCommand": "cd next_frontend && npm install",
  "framework": "nextjs",
  "regions": ["iad1"],
  "env": {
    "NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY": "@clerk-publishable-key",
    "CLERK_SECRET_KEY": "@clerk-secret-key",
    "NEXT_PUBLIC_LIVEKIT_URL": "@livekit-url",
    "NEXT_PUBLIC_BACKEND_URL": "@backend-url"
  }
}
